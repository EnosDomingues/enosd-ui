{
  "name": "@enosd-ui/react",
  "version": "1.0.0",
  "type": "module",
  "description": "A React component library designed for our projects, offering reusable UI components that adhere to our design system. This package includes base components like buttons, inputs, modals, and more, all styled according to our design tokens for seamless integration across applications.",
  "keywords": [
    "react",
    "ui-components",
    "design-system",
    "frontend",
    "react-components"
  ],
  "author": "Enos Domingues",
  "license": "MIT",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "scripts": {
    "build": "postcss src/index.css -o dist/index.css && tsup src/index.tsx --format esm,cjs --dts",
    "dev": "postcss src/index.css -o dist/index.css && tsup src/index.tsx --format esm,cjs --dts --watch"
  },
  "dependencies": {
    "@enosd-ui/tokens": "workspace:*",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.17"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^6.18.2",
    "@typescript-eslint/parser": "^6.18.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.2",
    "vite": "^4.5.0"
  }
}